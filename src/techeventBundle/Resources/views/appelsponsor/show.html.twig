{% extends 'base.html.twig' %}

{% block content %}
    <h1>Appelsponsor</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ appelSponsor.id }}</td>
            </tr>
            <tr>
                <th>Role</th>
                <td>{{ appelSponsor.role }}</td>
            </tr>
            <tr>
                <th>Confirmation</th>
                <td>{{ appelSponsor.confirmation }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
